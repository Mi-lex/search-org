{
    "presets": [
        [
            "@babel/env",
            {
                "useBuiltIns": "usage",
                "corejs": { "version": 3 }
            }
        ],
        [ 
            "@babel/preset-react"
        ],
        [
            "@babel/preset-typescript"
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}